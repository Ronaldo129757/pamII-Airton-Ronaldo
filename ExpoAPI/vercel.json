{
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api"
      },
      {
        "source": "/(.*)",
        "destination": "/$1"
      }
    ]
  }